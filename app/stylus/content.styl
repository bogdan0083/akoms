				.cursor
					opacity 0
			.btn
				width 100%
			.cursor 
				sprite($cursor)
				position absolute
				animation cursorMove 2s infinite ease
				pointer-events none
				transition all 0.4s
		.front-img
			position absolute
			//left 0
			//top 0
			z-index 1
			height 0
			width 0
			border-radius 50%
			overflow hidden
			transition all 1s
			&.active
